<p id="notice"><%= notice %></p>

<h1>Influencers</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Handle</th>
      <th>Follower count</th>
      <th>External created at</th>
      <th>Created at</th>
      <th>Updated at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @influencers.each do |influencer| %>
      <tr>
        <td><%= influencer.id %></td>
        <td><%= influencer.handle %></td>
        <td><%= influencer.follower_count %></td>
        <td><%= influencer.external_created_at %></td>
        <td><%= influencer.created_at %></td>
        <td><%= influencer.updated_at %></td>
        <td><%= link_to 'Show', influencer %></td>
        <td><%= link_to 'Edit', edit_influencer_path(influencer) %></td>
        <td><%= link_to 'Destroy', influencer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Influencer', new_influencer_path %>
